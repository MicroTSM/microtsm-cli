{
  "name": "vue-mfe-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "single-spa-vite-cli-service dev",
    "dev.watch": "nodemon --exec \"pnpm dev\"",
