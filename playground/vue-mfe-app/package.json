    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "prettier": "3.5.3",
    "single-spa-vue": "^3.0.1",
    "typescript": "~5.8.0",
    "vite": "^6.2.4",
    "vue-tsc": "^2.2.8"
  }
